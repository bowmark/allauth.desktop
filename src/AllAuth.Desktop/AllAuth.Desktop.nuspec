<?xml version="1.0"?>
<package>
  <metadata>
    <id>AllAuth</id>
    <version>0.1.0</version>
    <authors>Bowmark Ltd</authors>
    <description>AllAuth</description>
  </metadata>
  <files>
    <!-- Files without an extension require a wildcard at the end -->
    <!-- otherwise a "Part URI cannot end with a forward slash" error is thrown -->
    <file src="..\..\build\release\AllAuth.Desktop\VERSION*" target="lib\net45\" />
    <file src="..\..\build\release\AllAuth.Desktop\*.dll" target="lib\net45\" />
    <file src="..\..\build\release\AllAuth.Desktop\*.exe" target="lib\net45\" exclude="**\*.vshost.*" />
    <file src="..\..\build\release\AllAuth.Desktop\*.ini" target="lib\net45\" />
  </files>
</package>
